<header id="header">
  <div class="inner">
    <% if (is_home()) { %>
      <h1 id="logo">
        <a href="<%- url_for() %>">
          <%= config.title %>
          <span><%= config.subtitle %></span>
        </a>
      </h1>
    <% } else { %>
      <p id="logo">
        <a href="<%- url_for() %>">
          <%= config.title %>
          <span><%= config.subtitle %></span>
        </a>
      </p>
    <% } %>
  </div>
</header>
